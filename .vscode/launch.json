{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CalBot",
      "request": "launch",
      "type": "dart",
      "program": "frontend/lib/main.dart",
      "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "Node Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/backend/node-server/src/server.js",
      // "envFile": "${workspaceFolder}/backend/.env",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Discord Bot",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/backend/bot-server/bots/discord_bot/main.py",
      "cwd": "${workspaceFolder}/backend/bot-server",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/backend/bot-server/.env",
      "justMyCode": true
    }
  ]
}